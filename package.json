{
  "name": "univer-sheet-start-kit",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "A starter kit for Univer Sheet",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@univerjs/core": "^0.1.3",
